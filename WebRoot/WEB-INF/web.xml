<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
	<display-name></display-name>
	<servlet>
		<servlet-name>RegServlet</servlet-name>
		<servlet-class>com.why.servlet.RegServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>InitServlet</servlet-name>
		<servlet-class>com.why.servlet.InitServlet</servlet-class>
		<load-on-startup>10</load-on-startup>
	</servlet>
	<servlet>
		<servlet-name>LoginServlet</servlet-name>
		<servlet-class>com.why.servlet.LoginServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>SubmitAskServlet</servlet-name>
		<servlet-class>com.why.servlet.SubmitAskServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>QuestionsServlet</servlet-name>
		<servlet-class>com.why.servlet.QuestionsServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ViewAskServlet</servlet-name>
		<servlet-class>com.why.servlet.ViewAskServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ViewUserServlet</servlet-name>
		<servlet-class>com.why.servlet.ViewUserServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ViewAllUserServlet</servlet-name>
		<servlet-class>com.why.servlet.ViewAllUserServlet</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>FollowUserServlet</servlet-name>
		<servlet-class>com.why.servlet.FollowUserServlet</servlet-class>
	</servlet>


	<servlet-mapping>
		<servlet-name>RegServlet</servlet-name>
		<url-pattern>/RegServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>LoginServlet</servlet-name>
		<url-pattern>/Login</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>SubmitAskServlet</servlet-name>
		<url-pattern>/SubmitAsk</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>QuestionsServlet</servlet-name>
		<url-pattern>/Questions</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ViewAskServlet</servlet-name>
		<url-pattern>/ViewAskServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ViewUserServlet</servlet-name>
		<url-pattern>/viewuser</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ViewAllUserServlet</servlet-name>
		<url-pattern>/viewalluser</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>FollowUserServlet</servlet-name>
		<url-pattern>/FollowUser</url-pattern>
	</servlet-mapping>
	
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
</web-app>
